<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage x:Class="Tizen.NUI.Examples.xStaticMarkupExtensionDemoPage"
  xmlns="http://tizen.org/Tizen.NUI/2018/XAML"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <TextLabel Text="TextLabel1" Position2D="50,50" Size2D="300,50" >
        <TextLabel.BackgroundColor>
            <x:StaticExtension Member="Color.Red" />
        </TextLabel.BackgroundColor>
    </TextLabel>

  <TextLabel Text="TextLabel2" Position2D="50,200" Size2D="300,50" >
    <TextLabel.BackgroundColor>
      <x:Static Member="Color.Red" />
    </TextLabel.BackgroundColor>
  </TextLabel>

  <TextLabel Text="TextLabel3" Position2D="50,400" Size2D="300,50" BackgroundColor="{x:StaticExtension Member=Color.Red}" />

  <TextLabel Text="TextLabel4" Position2D="50,600" Size2D="300,50" BackgroundColor="{x:StaticExtension Color.Red}" />
  
</ContentPage>
