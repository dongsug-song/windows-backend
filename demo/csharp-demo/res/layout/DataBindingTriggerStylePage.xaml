<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage x:Class="Tizen.NUI.Examples.TestStaticDynamicResourcePage"
  xmlns="http://tizen.org/Tizen.NUI/2018/XAML"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:System="clr-namespace:System;assembly=mscorlib">

  <ContentPage.Resources>
    <ResourceDictionary Source="IndependentResource.xaml" />
  </ContentPage.Resources>
  
  <ImageView Name="img1" ResourceUrl="*DemoRes*/images/ambient/default/picture_07.jpg" Position2D="47,52" Size2D="440,440" >
    <ImageView.BGImage>
      <x:Array Type="{x:Type x:Single?}">
        <x:Single-N>null</x:Single-N>
        <x:Single>1.0</x:Single>
        <x:Single>2.0</x:Single>
        <x:Single>3.0</x:Single>
        <x:Single>4.0</x:Single>
      </x:Array>
    </ImageView.BGImage>
  </ImageView>


  <ImageView Name="img1" ResourceUrl="{StaticResource urlKey}" Position2D="{DynamicResource positionKey}" />
</ContentPage>
