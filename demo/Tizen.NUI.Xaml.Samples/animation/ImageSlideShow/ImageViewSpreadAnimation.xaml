<Transition xmlns="http://tizen.org/Tizen.NUI/2018/XAML"  
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            Duration="3500"
            EndAction="Cancel"
			LoopCount="1"
            Name="imageviewSpreadAnimation">

	<Transition.Behaviors>
		<x:Array Type="{x:Type AnimationBehavior}">
			<AnimationBehavior Key="Opacity" Property="Opacity" DestValue="1.0" />
			<AnimationBehavior Key="Image0Scale" Property="Scale" DestValue="1.2,1.2,0" />
			<AnimationBehavior Key="Image0Position" Property="Position" DestValue="0,250,0" />
			<AnimationBehavior Key="Image1Position" Property="Position" DestValue="-324,-336,0" />
			<AnimationBehavior Key="Image2Position" Property="Position" DestValue="324,-432,0" />
			<AnimationBehavior Key="Image3Position" Property="Position" DestValue="-260,864,0" />
			<AnimationBehavior Key="Image4Position" Property="Position" DestValue="260,750,0" />
		</x:Array>
	</Transition.Behaviors>

 </Transition>