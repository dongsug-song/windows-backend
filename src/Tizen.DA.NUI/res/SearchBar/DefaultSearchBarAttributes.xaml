<AttributesContainer xmlns="http://tizen.org/Tizen.NUI/2018/XAML"  
                xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <AttributesContainer.XamlResources>
        <ColorSelector x:Key="DADefaultSearchInputFieldTextColor"
                        Normal="0, 0, 0, 1.0"
                        Pressed ="0, 0, 0, 1.0"
                        Disabled="0, 0, 0, 0.4"
                        />
    </AttributesContainer.XamlResources>
    
    <SearchBarAttributes
        x:Name="SearchBarAttributes"
        Space="56">

        <SearchBarAttributes.SearchBoxAttributes
            x:Name="SearchBoxAttributes">

            <InputFieldAttributes 
                x:Name="InputFieldAttributes">
            </InputFieldAttributes>

        </SearchBarAttributes.SearchBoxAttributes>

        <SearchBarAttributes.ResultListAttributes
                x:Name="ResultListAttributes">
            <ViewAttributes 
                x:Name="ViewAttributes"
                BackgroundColor="0.8, 0.8, 0.8, 0.8">
            </ViewAttributes>
        </SearchBarAttributes.ResultListAttributes>

    </SearchBarAttributes>

</AttributesContainer>
